{
  "presets": [
    "@babel/preset-typescript",
    ["@babel/preset-env", {
      "useBuiltIns": "usage",
      "corejs": 3,
      "targets": {
        "node": 14
      }
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "src": "./build/"
      }
    }]
  ]
}
